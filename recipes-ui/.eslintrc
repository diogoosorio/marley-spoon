{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier",
    "jsx-a11y",
    "react"
  ],
  "globals": {
    "process": true
  },
  "env": {
    "jest": true,
    "es6": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}